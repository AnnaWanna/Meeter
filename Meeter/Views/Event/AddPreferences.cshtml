@model IEnumerable<UserPreference>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Preference</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var euser in Model)
        {
            <tr>

                <td>@euser.User.FirstName</td>
                <td>@euser.User.LastName</td>

                @using (Html.BeginForm("AddPreference"))
                {

                    <div class="form-group">
                        @Html.EditorFor(esuer => euser.Type.Name, new { htmlAttributes = new { @class = "form-control" } })

                    </div>
                }
            </tr>
        }
    </tbody>
</table>
<link href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/themes/black-tie/jquery-ui.css" rel="Stylesheet">
<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/jquery-ui.js"></script>
<script>

    $(document).ready(function () {
        $('#esuer_Preferences').autocomplete({
            source: '@Url.Action("CurrentPreferenceAutocomplete")',
            select: function (event, ui) {
                //set tagids to save
                $("#Preference_Id").val(ui.item.id);
                $("#Preference_Type").val(ui.item.type);

                //Tags for display
                this.value = ui.item.value;
                return false;
            }
        });
    })

</script>
<link href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/themes/black-tie/jquery-ui.css" rel="Stylesheet">
<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/jquery-ui.js"></script>
<script>

    $(document).ready(function () {
        $('#User_UserName').autocomplete({
            source: '@Url.Action("CurrentMemberAutocomplete")',
            select: function (event, ui) {
                //set tagids to save
                $("#User_Id").val(ui.item.id);
                $("#User_FirstName").val(ui.item.firstname);
                $("#User_LastName").val(ui.item.lastname);
                $("#User_PhoneNumber").val(ui.item.phonenumber);
                $("#User_Email").val(ui.item.email);

                //Tags for display
                this.value = ui.item.value;
                return false;
            }
        });
    })

</script>